{
    "compilerOptions": {
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "module": "commonjs",
      "jsx": "react-jsx",
      "paths": {
          "#root/*": ["./src/*"],
          "#utils/*": ["./src/utils/*"]
      },
      "skipLibCheck": true,
      "strict": true,
      "strictPropertyInitialization": false,
      "target": "es5"
    }
  }