{
    "compilerOptions": {
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "module": "commonjs",
      "jsx": "react",
      "paths": {
          "#root/*": ["./src/*"]
      },
      "skipLibCheck": true,
      "strict": true,
      "strictPropertyInitialization": false,
      "target": "es5"
    }
  }